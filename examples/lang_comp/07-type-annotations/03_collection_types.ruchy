// LANG-COMP-007: Type Annotations - Collection Types
// Demonstrates type annotations for arrays and tuples

// Array with type annotation
let numbers: [i32; 5] = [1, 2, 3, 4, 5]
let floats: [f64; 3] = [1.1, 2.2, 3.3]

// Tuple with type annotation
let pair: (i32, i32) = (10, 20)
let coords: (f64, f64) = (3.14, 2.71)

// Accessing typed collections
let first = numbers[0]
let x = coords.0
let y = coords.1

println(numbers)
println(floats)
println(pair)
println(first)
println(x)
println(y)
