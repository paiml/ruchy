// LANG-COMP-008: Methods - Integer Methods
// Demonstrates method calls on integer values

let num = 42

// Conversion methods
let as_string = num.to_string()
let as_float = num as f64

// Integer operations via methods
let negative: i32 = -5
let abs_val = negative.abs()

let power = (2i32).pow(3)  // Type suffix enables method calls on literals

// Output
println(num)
println(as_string)
println(as_float)
println(abs_val)
println(power)
