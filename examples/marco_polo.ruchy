fun main() {
    let command = "marco" in
    match command {
        "marco" => marco(),
        "polo" => polo("World"),
        "play" => play(3),
        _ => help()
    }
}

fun marco() {
    "Marco!"
}

fun polo(name: String) -> String {
    "Polo from " + name + "!"
}

fun play(times: i32) {
    let i = 1 in
    if i <= times {
        marco()
    } else {
        ()
    }
}

fun help() {
    "Usage: marco-polo [marco|polo NAME|play TIMES]"
}